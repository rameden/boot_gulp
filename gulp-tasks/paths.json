{
  "fonts": "assets/fonts/",
  "scripts": {
    "src": "assets/js/src/**/*.js",
    "build": "dist/assets/js/"
  },
  "styles": {
    "src": "assets/css/src/**/*.less",
    "build": "dist/assets/css/"
  },
  "media": {
    "src": "assets/media/src/**/*",
    "build": "dist/assets/media/"
  },
  "templates": {
    "src": "assets/templates/**/*",
    "build": "dist/"
  },
  "svg": {
    "src": "assets/svg/src/*.svg",
    "build": "dist/assets/svg/"
  }
}
